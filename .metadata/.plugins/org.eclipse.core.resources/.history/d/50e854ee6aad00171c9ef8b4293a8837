package com.technoangel.hackerrank;

import org.junit.Test;
import static org.junit.Assert;

public class BirthdayCake {

	public Integer solvesInvites(int va, int vA, int vB) {
		return 2;
	}
	
	@Test
	public void verifyCakeSlices() {
		int va = 1;
		int vA = 5;
		int vB = 10;
		
		BirthdayCake bc = new BirthdayCake();
		Integer invites = bc.solvesInvites(va, vA, vB);
		Assert.assertEquals(invites, 2);
	}
	
	public static void main(String[] args) {
	
	}

}
