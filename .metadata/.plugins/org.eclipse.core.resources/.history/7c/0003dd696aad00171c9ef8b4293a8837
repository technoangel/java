package com.technoangel.hackerrank;

public class BirthdayCake {

	public static void main(String[] args) {
		@Test
		public void verifyCakeSlices() {
			int va = 1;
			int aA = 5;
			int vB = 10;
			
			BirthdayCake bc = new BirthdayCake();
			assertEqual(bc.solveInvites(va, vA, vB), 2);
		}
	}

}
